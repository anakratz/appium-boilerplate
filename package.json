{
  "name": "webdriverio6-appium-mocha-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project to run native and browser for android and ios using mocha with page object pattern",
  "scripts": {
    "android.browserstack.app": "./node_modules/.bin/wdio ./config/browserstack/wdio.android.bs.app.conf.js",
    "android.sauce.rdc.app": "./node_modules/.bin/wdio ./config/saucelabs/wdio.android.rdc.app.conf.js",
    "android.app": "./node_modules/.bin/wdio ./config/wdio.android.app.conf.js",
    "android.browser": "./node_modules/.bin/wdio ./config/wdio.android.browser.conf.js",
    "ios.app": "./node_modules/.bin/wdio ./config/wdio.ios.app.conf.js",
    "ios.sauce.rdc.app": "./node_modules/.bin/wdio ./config/saucelabs/wdio.ios.rdc.app.conf.js",
    "ios.browserstack.app": "./node_modules/.bin/wdio ./config/browserstack/wdio.ios.bs.app.conf.js",
    "ios.browser": "./node_modules/.bin/wdio ./config/wdio.ios.browser.conf.js",
    "lint": "eslint .",
    "allure": "allure generate ./reports/allure/allure-results && allure open --clean"
  },
  "keywords": [
    "Appium",
    "Mocha",
    "webdriverio",
    "wdio",
    "mobile automation",
    "native apps",
    "mobile browser"
  ],
  "author": "Arjun Annamalai - arjun1120@gmail.com",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "framework": "0.0.0-reborn",
    "milsymbol": "^2.0.0",
    "mocha": "^8.4.0",
    "wdio": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/register": "^7.13.16",
    "@babel/traverse": "^7.10.4",
    "@babel/types": "^7.10.4",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.12.1",
    "@wdio/browserstack-service": "^6.6.7",
    "@wdio/cli": "^6.1.24",
    "@wdio/local-runner": "^6.1.24",
    "@wdio/mocha-framework": "^6.1.19",
    "@wdio/sauce-service": "^6.1.16",
    "@wdio/spec-reporter": "^6.1.23",
    "@wdio/sync": "^6.1.14",
    "allure-commandline": "^2.13.0",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^83.0.0",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-wdio": "^6.6.0",
    "jasmine-allure-reporter": "^1.0.2",
    "node-fetch": "^2.6.1",
    "wdio-chromedriver-service": "^6.0.3",
    "wdio-json-reporter": "^2.0.0",
    "webdriverio": "^6.12.1"
  }
}
